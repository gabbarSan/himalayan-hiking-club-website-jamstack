<h4>Testimonials</h4>
<div class="carousel">
    {%- for testimonial in testimonials -%}
    <div>
    <blockquote class="testimonial">
        <p>{{ testimonial.userTestimonialDetail | safe }}</p>
    </blockquote>
    <div class="testimonial-arrow-down">
    </div>
    <div class="testimonial-author">
        <div class="img-thumbnail img-thumbnail-small">
        <img class="img-circle" src="{{ testimonial.userProfileImage }}" alt="">
        </div>
        <p><strong>"{{ testimonial.userName | safe }}"</strong><span>{{ testimonial.userRelationship | safe }}</span></p>
    </div>
    </div>
{%- endfor -%}
</div>